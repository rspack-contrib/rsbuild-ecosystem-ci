  },
  "devDependencies": {
    "@antfu/ni": "^0.23.2",
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^18.19.122",
    "@types/semver": "^7.7.0",
    "nano-staged": "^0.8.0",
    "semver": "^7.7.2",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.20.3",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@10.14.0",
